{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "qbt",
  "mainBinaryName": "qbt",
  "version": "../package.json",
  "identifier": "com.gmail.karoyqiu.qbt",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src 'self'; style-src 'self' 'nonce-Eyb2JqlROQDm6V2LGWjuj'; connect-src ipc: http://ipc.localhost; img-src 'self' data: https:"
    },
    "windows": [
      {
        "title": "qBittorrent",
        "width": 1200,
        "height": 700,
        "center": true,
        "visible": false,
        "useHttpsScheme": true
      }
    ]
  }
}
